{
    "AllowFileSelectionDialogs": true,
    "AlternateErrorPagesEnabled": false,
    "AlwaysOpenPdfExternally": false,
    "AutofillCreditCardEnabled": false,
    "AutofillAddressEnabled": false,
    "BackgroundModeEnabled": false,
    "BlockThirdPartyCookies": true,
    "BrowserAddPersonEnabled": false,
    "BrowserGuestModeEnabled": false,
    "BrowserSignin": 1,
    "BuiltInDnsClientEnabled": false,
    "CertificateTransparencyEnforcementDisabledForCas": [],
    "CertificateTransparencyEnforcementDisabledForUrls": [ 'internal' ],
    "CloudPrintProxyEnabled": false,
    "ComponentUpdatesEnabled": true,
    "CookiesAllowedForUrls": [
        "https://www.example.com",
        "[*.]example.edu",
        "[*.]internal",
        "https://drive.google.com",
        "https://[*.]googleusercontent.com"
    ],
    "DefaultBrowserSettingEnabled": false,
    "DefaultCookiesSetting": 4,
    "DefaultGeolocationSetting": 2,
    "DefaultNotificationsSetting": 2,
    "DefaultWebBluetoothGuardSetting": 2,
    "DisablePrintPreview": false,
    "DownloadRestrictions": 1,
    "ExtensionInstallBlocklist": [{% for entry in chrome_extension_blocklist %}"{{ entry }}"{% if not loop.last %},{% endif %}{% endfor %}],
    "ExtensionInstallAllowlist": [{% for entry in chrome_extension_allowlist %}"{{ entry }}"{% if not loop.last %},{% endif %}{% endfor %}],
    "ExtensionInstallForcelist": [{% for entry in chrome_extension_force %}"{{ entry }}"{% if not loop.last %},{% endif %}{% endfor %}],
{% if chrome_blockedhost_extensions_list is defined or chrome_allowedhost_extensions_list is defined %}
    "ExtensionSettings": {
{% if chrome_blockedhost_extensions_list is defined and chrome_blockedhost_extensions_list %}
{% for i in chrome_blockedhost_extensions_list %}
        "{{ i.extension }}": {
            "runtime_blocked_hosts": [
{% for h in i.runtime %}
                "{{ h }}",
{% endfor %}
            ]
        }{% if not loop.last or (chrome_allowedhost_extensions_list is defined and chrome_allowedhost_extensions_list) %},{% endif %}

{% endfor %}
{% endif %}
{% if chrome_allowedhost_extensions_list is defined and chrome_allowedhost_extensions_list %}
{% for i in chrome_allowedhost_extensions_list %}
        "{{ i.extension }}": {
            "runtime_allowed_hosts": [
{% for h in i.runtime %}
                "{{ h }}",
{% endfor %}
            ]
        }{% if not loop.last %},{% endif %}

{% endfor %}
{% endif %}
    },
{% endif %}
    "HideWebStoreIcon": true,
    "ImportAutofillFormData": false,
    "ImportBookmarks": false,
    "ImportHistory": false,
    "ImportHomepage": false,
    "ImportSavedPasswords": false,
    "ImportSearchEngine": false,
    "IncognitoModeAvailability": 1,
    "MetricsReportingEnabled": false,
    "NetworkPredictionOptions": 2,
    "PasswordManagerEnabled": false,
    "PromotionalTabsEnabled": false,
    "ProxySettings": {
      "ProxyBypassList": "https://www.example1.com,https://www.example2.com,https://internalsite/",
      "ProxyMode": "direct",
      "ProxyServer": "123.123.123.123:8080"
    },
    "RelaunchNotification": 2,
    "RelaunchNotificationPeriod": 86400000,
    "RemoteAccessHostAllowGnubbyAuth": false,
    "RemoteAccessHostAllowClientPairing": false,
    "RemoteAccessHostRequireCurtain": true,
    "RemoteAccessHostFirewallTraversal": false,
    "RemoteAccessHostAllowRelayedConnection": false,
    "SearchSuggestEnabled": false,
    "ShowAppsShortcutInBookmarkBar": false,
    "SpellCheckServiceEnabled": false,
    "TranslateEnabled": false,
    "WPADQuickCheckEnabled": false
}
